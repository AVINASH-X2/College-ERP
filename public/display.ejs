<!DOCTYPE html>
<html>

<head>
  <!-- Link to your CSS file -->
  <link rel="stylesheet" type="text/css" href="../css/style.css">
</head>
<style>
  .info-box {
    border: 2px solid #009866;
    padding: 20px;
  }
  body{
    background: url(backgrnd[2].gif);
  }
</style>

<body>
  <div>
    <!-- Include your HTML content directly here -->
    <div style="margin-left: 137px;
      margin-top: 50px;
      border: none;
      color: #009866;
      border-bottom: 2px solid #009866;
      margin-right: 132px;">
      <h1>Addmision Management Dashboard</h1>
    </div>
    <div style="margin-left: 140px;
      margin-right: 140px;
      margin-top: 98px;">
      <% students.forEach((student, index)=> { %>
        <div class="info-box">
          <div class="student-row">
            <span class="label-cell"><strong>Name:</strong></span>
            <span class="value-cell">
              <%= student.username %>
            </span>
          </div>
          <div class="student-row">
            <span class="label-cell"><strong>Email:</strong></span>
            <span class="value-cell">
              <%= student.email %>
            </span>
          </div>
          <div class="student-row">
            <span class="label-cell"><strong>Gender:</strong></span>
            <span class="value-cell">
              <%= student.gender %>
            </span>
          </div>
          <div class="student-row">
            <span class="label-cell"><strong>Program:</strong></span>
            <span class="value-cell">
              <%= student.department %>
            </span>
          </div>
          <div class="student-row">
            <span class="label-cell"><strong>Semester:</strong></span>
            <span class="value-cell">
              <%= student.semester %>
            </span>
          </div>

        </div>
    </div>
    <% if (index !==students.length - 1) { %>
      <hr class="collection-separator">
      <% } %>
        <% }); %>
  </div>
  <div style="margin-left: 143px;
      margin-top: 36px; margin-bottom:50px">
    <a style="text-decoration-line: none;
        border: none;
        font-size: 20px;
        border-bottom: 2px solid #009866;
        padding-bottom:10px;
        color: #009866;
      " href="/student">Add Student</a>
    <!-- <a style="text-decoration-line: none;
        border: none;
        font-size: 20px;
        border-bottom: 2px solid #009866;
        padding-bottom: 10px;
        color: #009866; float:right; margin-right: 143px;
      " href="/student">Back</a> -->
  </div>
  </div>
</body>

</html>